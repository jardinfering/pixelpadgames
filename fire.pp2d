{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"\nself.background=object_new('Background')\nself.background.x=0\nself.background.y=0\nself.player=object_new('Fire')\nself.player.x=0\nself.player.y=0\nself.timer = 0\nself.score = 0\nself.text = new_text(self.score,400,375)\nself.text.color = \"red\"\nself.txt = \"\"\nself.text.fontSize=100","loop":"import random\nself.timer = self.timer + 1\n\nif self.timer>=30:\n    platform=object_new('Block')\n    platform.x = random.randint(-300, 400)\n    platform.y = random.randint(-300, 400)\n    self.timer=0\n\n"}},{"Fire":{"type":"object script","start":"#Fire start\nself.sprite=sprite_new('fire.png')\nself.scaleX=0.5\nself.scaleY=0.5\n","loop":"#Fire loop\nsprite_sheet = sprite_new('fire.png', 3,3)\nanim = animation_new(sprite_sheet, 20, 0, 7)\nanimation_set(self, anim)\n\nif key_is_pressed('w'): \n  self.y=self.y + 10\nif key_is_pressed('s'): \n  self.y=self.y - 10\nif key_is_pressed('a'): \n  self.x=self.x - 10\nif key_is_pressed('d'): \n  self.x=self.x + 10"}},{"Background":{"type":"object script","start":"#Background start\nself.sprite=sprite_new('background.gif')\nself.scaleX = 2\nself.scaleY = 2\n","loop":"#Background loop\n\n"}},{"Block":{"type":"object script","start":"#Block start\nself.sprite=sprite_new('block.png')\nself.scaleX = 0.05\nself.scaleY = 0.05\n","loop":"#Block loop\nif get_collision(self, 'Fire'): \n    destroy(self)\n    game.score = game.score + 1\n    game.text.text=str(game.score)"}}],"room":[],"texture":[{"fire.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794757.1712025699.fire.png"}},{"background.gif":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794757.1712025736.background.gif"}},{"block.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794757.1712026039.block.png"}}],"sound":[],"function":[]},"blockAssets":{"script":[{"Game":{"ty