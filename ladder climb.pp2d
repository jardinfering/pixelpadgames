{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"background=object_new('Background')\nbackground.x=0\nladder=object_new('Ladder')\nladder.x=250\nladder.y=-140\nplayer=object_new('Player')\nfloor=object_new('Floor')\nfloor.y=-350\nwall=object_new('Wall')\nwall.x=-700\nwall=object_new('Wall')\nwall.x=700\n","loop":"\n\n"}},{"Player":{"type":"object script","start":"#Player start\nself.sprite=sprite_new('player.png')\nself.scaleX = 0.1\nself.scaleY = 0.1\nself.velocityX = 0\nself.velocityY = 0\nself.gravityForce = 2.55\nself.ground=False\nself.climbing=False","loop":"#Player loop\n\nself.x = self.x + self.velocityX\nself.y = self.y + self.velocityY-self.gravityForce\n\n\n#if collision_check(self, 'Ladder') :\n#    self.y+=1\n\nif key_is_pressed('d'): \n   self.velocityX=self.velocityX + 0.1\n\n  \nif key_is_pressed('a'): \n   self.velocityX=self.velocityX - 0.1\n  \n#if key_is_pressed('w'):\n   #self.velocityY=self.velocityY + 0.1\n\nif key_is_pressed('s') and collision_check(self, 'Floor'):\n    self.velocityY=self.velocityY - 0.1\n    self.ground = True\n\n\nif key_is_pressed('w') and collision_check(self, 'Ladder'):\n    self.y+=5\n    self.climbing=True\nelse:\n    gravityForce=2.55\n    self.climbing=False\n\n\nif self.climbing == True:\n    self.gravityForce=0\n\nif self.climbing == False:\n    self.gravityForce = 2.55\n\n\nif collision_check(self,'Floor'):\n    self.gravityForce=0\n    self.velocityY = 0\n    self.ground=True\n    self.climbing=False\nelse:\n    gravityForce=2.55\n    self.ground=False\n\nif collision_check(self, 'Wall'):\n       self.velocityX = -self.velocityX \n\n\n"}},{"Background":{"type":"object script","start":"#Background start\nself.sprite=sprite_new('background.gif')\nself.scaleX = 2.5\nself.scaleY = 2.5\n\n","loop":"#Background loop\n\n"}},{"Floor":{"type":"object script","start":"#Floor start\nself.sprite=sprite_new('floor.png')\nself.scaleX = 1.5\nself.scaleY = 0.1\n","loop":"#Floor loop\n\n"}},{"Ladder":{"type":"object script","start":"#Ladder start\nself.sprite=sprite_new('ladder.png')\nself.scaleX = 0.2\nself.scaleY = 0.2\n","loop":"#Ladder loop\n\n"}},{"Wall":{"type":"object script","start":"#Wall start\nself.sprite=sprite_new('wall.png')\nself.scaleX = 0.5\nself.scaleY = 5\n","loop":"#Wall loop\n\n"}}],"room":[],"texture":[{"wall.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1702142.1707602140.wall.png"}},{"background.gif":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1702142.1707602255.background.gif"}},{"player.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1702142.1707602282.player.png"}},{"floor.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1702142.1707602304.floor.png"}},{"ladder.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1702142.1707602347.ladder.png"}}],"sound":[],"function":[]},"blockAssets":{"script":[{"Game":{"type":"game script","start":{"blocks":{"blocks":[{"type":"start_block","id":"startBlock","x":100,"y":100,"fields":{"NAME":"Game Start"}}]}},"loop":{"blocks":{"blocks":[{"type":"loop_block","id":"loopBlock","x":100,"y":100,"fields":{"NAME":"Game Loop"}}]}}}}],"room":[],"texture":[],"sound":[],"function":[],"gameVars":[]},"projectSettings":{"type":"monaco"}}