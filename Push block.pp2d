{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"self.player=object_new('Background')\n\nself.player.y=0\nself.player=object_new('Player')\nself.player.x=-570\nself.player.y=0\nself.block=object_new('Block')\nself.block.x=200\nself.block.y=0\n\n","loop":"\n\n"}},{"Player":{"type":"object script","start":"#Player start\nself.sprite=sprite_new('player.png')\nself.scaleX = 0.08\nself.scaleY = 0.08\n","loop":"#Player loop\nif key_is_pressed('w'): \n  self.y=self.y + 10\nif key_is_pressed('s'): \n  self.y=self.y - 10\nif key_is_pressed('a'): \n  self.x=self.x - 10\nif key_is_pressed('d'): \n  self.x=self.x + 10\n\n\nif self.x > 750:\n    self.x = 750\nif self.x < -750:\n    self.x = -750\nif self.y > 400:\n    self.y = 400\nif self.y < -400:\n    self.y = -400\n\nif key_was_pressed('L'): \n    block=object_new('Bullet')\n    block.x=self.x\n    block.y=self.y\n\nif key_was_pressed(' '): \n    block=object_new('Block')\n    block.x=self.x + 150\n    block.y=self.y"}},{"Block":{"type":"object script","start":"#Block start\nself.sprite=sprite_new('block.png')\nself.scaleX = 2.75\nself.scaleY = 2.75\n","loop":"#Block loop\n\nif get_collision(self, 'Bullet'):\n    destroy(self)\n\n\nif collision_check(self, 'Player'):\n    if key_is_pressed('d'):\n        self.x = self.x + 10\n    if key_is_pressed('a'):\n        self.x = self.x - 10\n    if key_is_pressed('w'):\n        self.y = self.y + 10\n    if key_is_pressed('s'):\n        self.y = self.y - 10\n\nif self.x > 500:\n    self.x = 500\nif self.x < -500:\n    self.x = -500\nif self.y > 250:\n    self.y = 250\nif self.y < -250:\n    self.y = -250\n\n"}},{"Bullet":{"type":"object script","start":"#Bullet start\nself.sprite=sprite_new('player.png')\nself.scaleX = 0.08\nself.scaleY = 0.08\n\n","loop":"#Bullet loop\nself.x = self.x + 30\nif get_collision(self, 'Block'):\n    destroy(self)\n"}},{"Background":{"type":"object script","start":"#Background start\nself.sprite=sprite_new('background.gif')\nself.scaleX = 2\nself.scaleY = 2\n","loop":"#Background loop\n\n"}}],"room":[],"texture":[{"block.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794647.1711940380.block.png"}},{"player.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794647.1711940389.player.png"}},{"background.gif":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794647.1712007474.background.gif"}},{"bullet.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794647.1712007487.bullet.png"}}],"sound":[],"function":[]},"blockAssets":{"script":[{"Game":{"type":"game script","start":{"blocks":{"blocks":[{"type":"start_block","id":"startBlock","x":100,"y":100,"fields":{"NAME":"Game Start"}}]}},"loop":{"blocks":{"blocks":[{"type":"loop_block","id":"loopBlock","x":100,"y":100,"fields":{"NAME":"Game Loop"}}]}}}}],"room":[],"texture":[],"sound":[],"function":[],"gameVars":[]},"projectSettings":{"type":"monaco"}}