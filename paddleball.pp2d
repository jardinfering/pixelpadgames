{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"#self.background=object_new('Background')\n#self.background.x=0\n#self.background.y=0\nself.paddle=object_new('Paddle')\nself.paddle.x=-600\nself.paddle.y=-200\nself.paddle=object_new('Paddle2')\nself.paddle.x=600\nself.paddle.y=-200\nself.ball=object_new('Ball')\nself.ball.x=0\nself.ball.y=0\nself.wall=object_new('Wall')\nself.wall.x=0\nself.wall.y=350\nself.wall=object_new('Wall2')\nself.wall.x=0\nself.wall.y=-350\nself.score = 0\nself.score1 = 0\nself.text = new_text(self.score1,400,350)\nself.text.color = \"red\"\nself.txt = \"\"\nself.text.fontSize=100\nself.text = new_text(self.score,-400,350)\nself.text.color = \"blue\"\nself.txt = \"\"\nself.text.fontSize=100","loop":"\n\n\n\n"}},{"Paddle":{"type":"object script","start":"#paddle start\nself.sprite=sprite_new('paddle.png')\nself.scaleX = 0.5\nself.scaleY = 5\n","loop":"#paddle loop\nif key_is_pressed('w'): \n  self.y=self.y + 10\nif key_is_pressed('s'): \n  self.y=self.y - 10\n\nif self.y > 280:\n    self.y = 280\nif self.y < -280:\n    self.y = -280"}},{"Paddle2":{"type":"object script","start":"#Paddle2 start\nself.sprite=sprite_new('paddle.png')\nself.scaleX = 0.5\nself.scaleY = 5\n","loop":"#Paddle2 loop\nif key_is_pressed('arrowUp'): \n  self.y=self.y + 10\nif key_is_pressed('arrowDown'): \n  self.y=self.y - 10\n\nif self.y > 280:\n    self.y = 280\nif self.y < -280:\n    self.y = -280\n"}},{"Ball":{"type":"object script","start":"#Ball start\nself.sprite=sprite_new('ball1.png')\nself.scaleX = 2\nself.scaleY = 2\nself.velocityX = -2\nself.velocityY = 2\n","loop":"#Ball loop\n\nself.x = self.x + self.velocityX\nself.y = self.y + self.velocityY\n\nif get_collision(self, 'Paddle'):\n    self.velocityX = -self.velocityX + 1\n    self.velocityY = self.velocityY - 1\n\nif get_collision(self, 'Paddle2'):\n    self.velocityX = -self.velocityX - 1\n    self.velocityY = self.velocityY + 1\n\n\nif get_collision(self, 'Wall'):\n    self.velocityY = -self.velocityY - 1\n\nif get_collision(self, 'Wall2'):\n    self.velocityY = -self.velocityY + 1\n\nif self.x <= -700:\n    self.x = 0 \n    self.y = 0\n    self.velocityX = 2\n    self.velocityY = 2\n    score()\n    print(score)\n#    game.score = game.score + 1\n#    game.text.text=str(game.score)\n\nif self.x >= 700:\n    self.x = 0 \n    self.y = 0\n    self.velocityX = -2\n    self.velocityY = -2\n    score1()\n#    game.score1 = game.score1 + 1 \n#    game.text.text=str(game.score1)"}},{"Wall":{"type":"object script","start":"#Wall start\nself.sprite=sprite_new('wall.png')\nself.scaleX = 50\nself.scaleY = 1\n","loop":"#Wall loop\n\n"}},{"Wall2":{"type":"object script","start":"#Wall2 start\nself.sprite=sprite_new('wall.png')\nself.scaleX = 50\nself.scaleY = 1\n","loop":"#Wall2 loop\n\n"}}],"room":[],"texture":[{"wall.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794759.1712462184.wall.png"}},{"paddle.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794759.1712462206.paddle.png"}},{"ball1.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1794759.1712462819.ball1.png"}}],"sound":[],"function":[]},"blockAssets":{"script":[{"Game":{"type":"game script","start":{"blocks":{"blocks":[{"type":"start_block","id":"startBlock","x":100,"y":100,"fields":{"NAME":"Game Start"}}]}},"loop":{"blocks":{"blocks":[{"type":"loop_block","id":"loopBlock","x":100,"y":100,"fields":{"NAME":"Game Loop"}}]}}}}],"room":[],"texture":[],"sound":[],"function":[],"gameVars":[]},"projectSettings":{"type":"monaco"}}