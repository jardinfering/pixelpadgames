{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"background=object_new('Background')\nbackground.x=40\nPlayer=object_new('Player')\nPlayer.x=-300\nPlayer.y=-100\nfloor=object_new('Floor')\nfloor.y=-300\n\n","loop":"\n\n"}},{"Player":{"type":"object script","start":"#Player start\nself.scaleX=3\nself.scaleY=3\nself.timer=0\n\n\n ","loop":"#Player loop\n\nself.timer=self.timer+1\nsprite_sheet = sprite_new('player.png', 3,3)\nanim = animation_new(sprite_sheet, 20, 0, 7)\nanimation_set(self, anim)\n\nif self.timer >= 20:\n  ball=object_new('Ball')\n  ball.y=-150\n  ball.x=self.x + 10\n  self.timer=0\n\n"}},{"Background":{"type":"object script","start":"#background start\nself.sprite=sprite_new('background.gif')\nself.scaleX = 2.5\nself.scaleY = 2.5\n","loop":"#background loop\n\n"}},{"Floor":{"type":"object script","start":"#Floor start\nself.sprite=sprite_new('floor.gif')\nself.scaleX = 10\nself.scaleY = 2\n","loop":"#Floor loop\n\n"}},{"Ball":{"type":"object script","start":"#Ball start\nself.sprite=sprite_new('ball.png')\nself.scaleX = 3\nself.scaleY = 3\nself.gravityForce = 1.55\nself.velocityX = 0\nself.velocityY = 0","loop":"#Ball loop\nself.y = self.y + self.velocityY-self.gravityForce\nself.x=self.x+7\n\nif collision_check(self,'Floor'):\n    self.velocityY = self.velocityY + self.gravityForce\n\nif self.y >= -100:\n    self.velocityY=-self.gravityForce"}}],"room":[],"texture":[{"background.gif":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1600748.1705785540.background.gif"}},{"ball.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1600748.1705785627.ball.png"}},{"floor.gif":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1600748.1705785701.floor.gif"}},{"player.png":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__ASSET__.12642.1600748.1706391565.player.png"}}],"sound":[],"functio