{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"room_set('tutorial')\n\n\n","loop":"\n\n"}},{"Player":{"type":"object script","start":"#Player start\nself.sprite=sprite_new('player.png')\nself.scaleX = 0.05\nself.scaleY = 0.05\nself.collide = False\nself.velocityX = 0\nself.velocityY = 0\nself.maxVelocity = 0.3\n\n","loop":"#Player loop\nself.x=self.x-2\n\nif key_is_pressed('w'): \n  self.y=self.y + 4\nif key_is_pressed('s'): \n  self.y=self.y - 4\nif key_is_pressed('a'): \n  self.x=self.x - 4\nif key_is_pressed('d'): \n  self.x=self.x + 4\n\nif self.collide == True:\n    self.x = self.x - 8\n\n#if get_collision(self, 'Platform'):\n    #self.x = Platform.x - 20\n    \n\nif get_collision(self, 'Platform'):\n    self.collide = True\nelse:\n    self.collide = False\n\n#if self.y > Platform.y and get_collision(self. 'Platform'):\n   # self.x = self.x + 4\n\nif get_collision(self, 'one'): \n    room_set('level1')\n\nif get_collision(self, 'two'): \n    room_set('level2')\n\nif get_collision(self, 'three'): \n    room_set('level3')\n\nif get_collision(self, 'four'): \n    room_set('level4')\n\nif get_collision(self, 'five'): \n    room_set('boss')\n\nif get_collision(self, 'help'): \n    room_set('meetfriend')\n\nif get_collision(self, 'enemy'): \n    room_set('friendhelp')\n\nif get_collision(self, 'finish'): \n    room_set('end')\n\nif get_collision(self, 'Wall'): \n    destroy(self)\n    room_set('tutorial')\n\nif get_collision(self, 'Floor'):\n    self.y = self.y + 20\n\nif get_collision(self, 'Roof'):\n    self.y = self.y - 20\n\nif get_collision(self, 'Bullet'): \n    destroy(self)\n    room_set('tutorial')\n"}},{"FakeFriend":{"type":"object script","start":"#fakeFriend start\nself.sprite=sprite_new('fakefriend.png')\nself.scaleX = 0.09\nself.scaleY = 0.09\n","loop":"#fakeFriend loop\n\n"}},{"Background":{"type":"object script","start":"#Background start\nself.sprite=sprite_new('background.gif')\nself.scaleX = 2\nself.scaleY = 2\n","loop":"#Backgro