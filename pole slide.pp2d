{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"background=object_new('Background')\nbackground.x=70\n\nfloor=object_new('Floor')\nfloor.x=400\nfloor.y=-360\nwall=object_new('Wall')\nwall.x=-700\nwall=object_new('Wall')\nwall.x=700\nself.pole=object_new('Pole')\nself.pole.x=200\nself.pole.y=-130\nself.player=object_new('Player')\nself.player.x=-350\nself.player.y=-300\n\n","loop":"if get_collision(self.player, self.pole) and self.player.y > -250:\n    self.player.x = self.pole.x \n"}},{"Player":{"type":"object script","start":"#Player start\n#Player start\nself.sprite=sprite_new('player.png')\nself.scaleX = 1\nself.scaleY = 1\nself.velocityX = 0\nself.velocityY = 0\n\nself.gravity = 0\n\n","loop":"self.y =  self.y  - 4\n\nif self.y <= -290:\n    self.y = -288\n\nif key_is_pressed('d'):\n    self.x = self.x + 10\n    \nif key_is_pressed('a'):\n    self.x = self.x - 10\n\nif key_is_pressed('w'):\n    self.y = self.y + 10\n\nif self.y >= 290:\n    self.y = 288\n\nif self.x >= 490:\n    self.x = 468\n    \nif self.x <= -490:\n    self.x = -468\n\n#if get_collision(self, 'Pole') is not get_collision(self, 'Floor'):\n #   self.x = Pole.x\n  #  self.y = self.y - 5\n\nif get_collision(self, 'Pole') and self.y >= -250:\n   self.y = self.y - 5\n\nif get_collision(self, 'Pole') and key_is_pressed('W'):\n   self.y = self.y - 5\n\n\nif get_collision(self, 'Wall'):\n    self.x = 300\n\nif get_collision(self, 'RightWall'):\n    self.x = 300"}},{"Floor":{"type":"object script","start":"#Floor start\nself.sprite=sprite_new('floor.png')\nself.scaleX = 3\nself.scaleY = 0.01\n","loop":"#Floor loop\n\n"}},{"Wall":{"type":"object script","start":"#Wall start\nself.sprite=sprite_new('wall.png')\nself.scaleX = 0.5\nself.scaleY = 5\n","loop":"#Wall loop\n\n"}},{"Background":{"type":"object script","start":"#Background start\nself.sprite=sprite_new('background.gif')\nself.scaleX = 2.5\nself.scaleY = 2.5\n","loop":"#Background loop\n\n"}},{"Pole":{"type":"object script","sta