ct script","start":"#tower2 start\nself.sprite = new_sprite(\"shooter.jpg\")\nself.scaleX = 1.5\nself.scaleY = 1.5\nself.timer = 0\nself.healthbar = Heathbar()\n","loop":"#tower2 loop\nself.timer = self.timer + 1\n\nif self.timer >= 100:\n    self.arrow=object_new('bullet2')\n    self.timer=0\n    self.arrow.x = self.x\n    self.arrow.y = self.y\n\n"}},{"Card1":{"type":"object script","start":"#Card1 start\nself.sprite = new_sprite(\"card1.png\")\nself.scaleX = 0.3\nself.scaleY = 0.3\nself.manacost=20\n","loop":"#Card1 loop\n\n\nimport random\n\nif mouse_was_pressed('left'):\n    if abs(self.x - mouse_x())< 50 and abs(self.y - mouse_y()) < 50:\n        if game.mana >= game.card1.manacost:\n            game.mana -= game.card1.manacost\n            card1move =  Card1move()\n            card1move.x = -450\n            card1move.y = -20 \n            card1move.healthbar2.x = card1move.x \n            card1move.healthbar2.y = card1move.y + 50 \n            card1move.team = \"eee\"\n            bridgeSelected = random.randint(1, 2)\n\n            if bridgeSelected == 1:\n                card1move.destinationX = game.bridge1.x\n                card1move.destinationY = game.bridge1.y\n            else:\n                card1move.destinationX = game.bridge.x\n                card1move.destinationY = game.bridge.y\n\n\n\n"}},{"Card1move":{"type":"object script","start":"#Card1move start\nself.sprite = new_sprite(\"card1image.png\")\nself.scaleX = 0.2\nself.scaleY = 0.2\nself.destinationX = 0\nself.destinationY = 0\nself.team = None\nself.healthbar2 = Healthbar2()\nself.healthbar2.scaleX = 0.1\nself.healthbar2.scaleY = 0.1\n","loop":"#Card1move loop\n\nimport random\nself.healthbar2.x = self.x\nself.healthbar2.y = self.y + 50     \n\nif self.x < self.destinationX:\n    self.x = self.x + 7.5\nelif self.x > self.destinationX:\n    self.x = self.x - 7.5\n\nif self.y < self.destinationY:\n    self.y = self.y + 5\nelif self.y > self.destinationY:\n    self.y = self.y - 5\n\nif self.x == self.destinationX and self.y == self.destinationY:\n    if self.team == \"eee\":\n       self.destinationX = game.enemy_castle.x\n       self.destinationY = game.enemy_castle.y - 50 \n    else:\n        self.destinationX = game.castle.x\n        self.destinationY = game.castle.y - 50\n\n#if collision_check(self, 'Enemy_castle'):\n #   destroy(self)\n\nif collision_check(self, 'Enemy'):\n    self.healthbar2.scaleX = self.healthbar2.scaleX-0.02\n\nif self.healthbar2.scaleX <= 0:\n    destroy(self.healthbar2)\n    destroy(self)\n    exp = explosion()\n    exp.x = self.x\n    exp.y = self.y \n\nif collision_check(self, 'bullet2'):\n    destroy(self.healthbar2)\n    destroy(self)\n\n\n    \n\n\n\n\n\n"}},{"Card2":{"type":"object script","start":"#Card2 start\nself.sprite = new_sprite(\"card2.png\")\nself.scaleX = 0.35\nself.scaleY = 0.35\nself.manacost=10\n","loop":"#Card2 loop\nimport random\n\nif mouse_was_pressed('left'):\n    if abs(self.x - mouse_x())< 50 and abs(self.y - mouse_y()) < 50:\n        if game.mana >= game.card1.manacost:\n            game.mana -= game.card1.manacost\n            card2move =  Card2move()\n            card2move.x = -450\n            card2move.y = -20\n            card2move.healthbar2.x = card2move.x \n            card2move.healthbar2.y = card2move.y + 50 \n            card2move.team = \"eee\"\n            bridgeSelected = random.randint(1, 2)\n\n            if bridgeSelected == 1:\n                card2move.destinationX = game.bridge1.x\n                card2move.destinationY = game.bridge1.y\n            else:\n                card2move.destinationX = game.bridge.x\n                card2move.destinationY = game.bridge.y\n\n"}},{"Card2move":{"type":"object script","start":"#Card2move start\nself.sprite = new_sprite(\"card2image.png\")\nself.scaleX = 0.6\nself.scaleY = 0.6\nself.destinationX = 0\nself.destinationY = 0\nself.team = None\nself.healthbar2 = Healthbar2()","loop":"#Card2move loop\nimport random\n\nif self.x < self.destinationX:\n    self.x = self.x + 7.5\nelif self.x > self.destinationX:\n    self.x = self.x - 7.5\n\nif self.y < self.destinationY:\n    self.y = self.y + 5\nelif self.y > self.destinationY:\n    self.y = self.y - 5\n\nif self.x == self.destinationX and self.y == self.destinationY:\n    if self.team == \"eee\":\n       self.destinationX = game.enemy_castle.x\n       self.destinationY = game.enemy_castle.y - 50 \n    else:\n        self.destinationX = game.castle.x\n        self.destinationY = game.castle.y - 50\n\n#if collision_check(self, 'Enemy_castle'):\n   # destroy(self)\n\n\nself.healthbar2.x = self.x\nself.healthbar2.y = self.y + 50     \n\nif collision_check(self, 'Enemy'):\n    self.healthbar2.scaleX = self.healthbar2.scaleX-0.01\n\nif self.healthbar2.scaleX <= 0:\n    destroy(self.healthbar2)\n    destroy(self)\n    exp = explosion()\n    exp.x = self.x\n    exp.y = self.y \n\nif collision_check(self, 'bullet2'):\n    destroy(self.healthbar2)\n    destroy(self)\n\n\n\n\n"}},{"Card3":{"type":"object script","start":"#Card3 start\nself.sprite = new_sprite(\"card3.jpg\")\nself.scaleX = 0.4\nself.scaleY = 0.4\nself.manacost=1000\n\n\n","loop":"#Card3 loop\nimport random\n\nif mouse_was_pressed('left'):\n    if abs(self.x - mouse_x())< 50 and abs(self.y - mouse_y()) < 50:\n        if game.mana >= game.card3.manacost:\n            game.mana -= game.card3.manacost\n            card3move =  Card3move()\n            card3move.x = -450\n            card3move.y = -20      \n            card3move.team = \"eee\"\n            #bridgeSelected = random.randint(1, 2)\n\n            #if bridgeSelected == 1:\n            #    card3move.destinationX = game.bridge1.x\n            #    card3move.destinationY = game.bridge1.y\n            #else:\n            #    card3move.destinationX = game.bridge.x\n            #    card3move.destinationY = game.bridge.y\n"}},{"Card3move":{"type":"object script","start":"#spaceship start\nself.sprite = new_sprite(\"spaceship.png\")\nself.scaleX 