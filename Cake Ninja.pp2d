{"pythonAssets":{"script":[{"Game":{"type":"game script","start":"background=object_new('Background')\r\ncake=object_new('Cake')\r\ncakespawner=object_new('cakespawner')\r\ncakespawner.y=-600\r\nslicer=object_new('Slicer')\r\n\r\nself.score = 0\r\nself.text = new_text(self.score,400,375)\r\nself.text.color = \"red\"\r\nself.txt = \"\"\r\nself.text.fontSize=100\r\n\r\n# self.text.y = 375\r\n# self.text.x = -85","loop":""}},{"Cake":{"type":"object script","start":"#Cake start\nimport random\n\nself.scaleX=0.25\nself.scaleY=0.2\nself.timer=0\nself.y=-600\nself.velocityY = random.randint(13, 17)\nself.velocityX = random.randint(-4, 4)\nrNum = random.randint(1,7)\n\nif (rNum == 1):\n    self.tag = \"cake1\"\n\nif (rNum == 2):\n    self.tag = \"cake2\"\n\nif (rNum == 3):\n    self.tag = \"cake3\"\n\nif (rNum == 4):\n    self.tag = \"cake4\"\n\nif (rNum == 5):\n    self.tag = \"cake1\"\n\nif (rNum == 6):\n    self.tag = \"cake2\"\n\nif (rNum == 7):\n    self.tag = \"cake3\"\n\nself.sprite = sprite_new(self.tag + \".jpg\")\n\n","loop":"\nself.velocityY = self.velocityY - 0.125\nself.y = self.y + self.velocityY\n\nself.x = self.x + self.velocityX\n\n\n    \n    \n\n\n\n"}},{"Background":{"type":"object script","start":"#Background start\nself.sprite=sprite_new('background.jpg')\nself.scaleX=0.975\nself.scaleY=1","loop":"#Background loop\n\n"}},{"cakespawner":{"type":"object script","start":"#cakespawner start\nself.timer=0\n\n\n","loop":"#cakespawner loop\nimport random\nself.timer = self.timer + 1\n#print(self.timer)\n\nif self.timer>=3:\n    cake1=object_new('Cake')\n    cake1.x = random.randint(-600, 600)\n    self.timer=0\n\n"}},{"Slicer":{"type":"object script","start":"#Slicer start\nself.sprite=sprite_new('Slicer1.png')\nself.scaleX=2\nself.scaleY=2\n\nself.previousX = 0\nself.previousY = 0\n\n","loop":"#Slicer loop\nself.x = mouse_x()\nself.y = mouse_y()\n\nself.speedX = self.x - self.previousX\nself.speedY = self.y - self.previousY\n\nif mouse_is_pressed(\"left\"):\n    for i in range(25):\n        self.posX = self.previousX + self.speedX * i/25\n        self.posY = self.previousY + self.speedY * i/25\n        self.trail = object_new(\"Slicetrail\")\n        self.trail.x = self.posX\n        self.trail.y = self.posY\n        if collision_check(self.trail, 'Cake'):\n            cake = collision_check(self.trail, 'Cake')\n            destroy(cake)\n            splash=object_new('Splash')\n            splash.x = cake.x\n            splash.y = cake.y\n            sprite_sheet = sprite_new('explosion.png', 2,2)\n            splash.scaleX=0.4\n            splash.scaleY=0.4\n            anim = animation_new(sprite_sheet, 60, 0, 3)\n            animation_set(splash, anim)\n            game.score = game.score + 1\n            game.text.text=str(game.score)\n            if cake.tag == \"cake2\":\n                 game.score == 0\n                 game.score = 0\n            \n\nself.previousX = self.x\nself.previousY = self.y\n\n\n\n\n\n"}},{"Slicetrail":{"type":"object script","start":"#Slicetrail start\nself.sprite=sprite_new('Slicer1.png')\nself.scaleX=2\nself.scaleY=2\n\nself.timer = 0\n","loop":"#Slicetrail loop\nself.timer = self.timer + 1\n\nif self.timer == 10:\n    destroy(self)\n    self.timer = 0\n\nself.scaleX = self.scaleX - 1/20\nself.scaleY = self.scaleY - 1/20\n"}},{"Splash":{"type":"object script","start":"#Splash start\nself.scaleX=0.01\nself.scaleY=0.01\nself.timer=0","loop":"#Splash loop\nself.timer= self.timer + 1\n\nif self.timer >=25:\n   destroy(self)\n   self.timer = 0"}}],"room":[],"texture":[{"background.jpg":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__PIXELPAD_ASSET__.12642.199519.background.jpg"}},{"cake1.jpg":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__PIXELPAD_ASSET__.12642.199519.cake1.jpg"}},{"cake2.jpg":{"type":"image","uri":"https://s3.us-west-1.amazonaws.com/media.pixelpad.io/__PIXELPAD_ASSET__.12642.199519.cake2.jpg"}},{"cake3.jpg":{